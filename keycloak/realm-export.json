{
  "id": "veille-realm-id",
  "realm": "VeilleRealm",
  "enabled": true,
  "clients": [
    {
      "clientId": "front",
      "enabled": true,
      "redirectUris": [
        "http://veille.localhost/*"
      ],
      "webOrigins": [
        "http://veille.localhost"
      ],
      "directAccessGrantsEnabled": true,
      "publicClient": true,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "86400",
        "refresh.token.lifespan": "2592000"
      }
    }

  ]
}
